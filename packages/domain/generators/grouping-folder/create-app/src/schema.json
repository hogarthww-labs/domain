{
  "$schema": "http://json-schema.org/schema",
  "$id": "CreateAppGroupingFolderGenerator",
  "title": "Create Application Grouping Folder Generator Schema",
  "cli": "nx",
  "type": "object",
  "description": "Creates application grouping folder",
  "properties": {
    "language": {
      "description": "The type of project",
      "type": "string",
      "enum": ["angular"],
      "default": "angular"
    },
    "name": {
      "description": "The name of the application",
      "type": "string"
    },
    "baseFolder": {
      "description": "The folder that the grouping folder will be created in",
      "type": "string"
    }
  },
  "required": ["name", "baseFolder"]
}
