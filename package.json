    "eslint-config-prettier": "8.3.0",
    "jest": "27.0.6",
    "path": "^0.12.7",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tslib": "^2.3.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
