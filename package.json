{
  "name": "@srleecode/domain",
  "description": "Nrwl nx plugin to allow operations at the domain level",
  "author": "Scott Lee",
  "version": "12.5.1",
  "license": "MIT",
  "homepage": "https://github.com/srleecode/domain",
  "scripts": {
    "nx": "nx",
    "build": "nx build domain",
    "test": "nx test domain",
    "lint": "nx lint domain",
    "e2e": "nx e2e domain-e2e",
    "update": "nx migrate latest",
    "debug-create": "node --inspect-brk ./node_modules/@nrwl/cli/bin/nx.js generate @srleecode/domain:create --application test-application --domain test-domain --prefix srlee --libraries data-access --addStorybookProject --dry-run",
    "publish": "npm run build && npm publish ./dist/packages/domain --access=public"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.3",
    "@angular-devkit/schematics": "^12.1.1",
