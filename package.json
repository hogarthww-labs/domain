    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@nrwl/angular": "12.6.2",
    "@nrwl/cypress": "12.6.2",
    "@nrwl/devkit": "12.6.2",
    "@nrwl/eslint-plugin-nx": "12.6.2",
    "@nrwl/jest": "12.6.2",
    "@nrwl/linter": "12.6.2",
    "@nrwl/node": "12.6.2",
    "@nrwl/nx-plugin": "12.6.2",
    "@nrwl/storybook": "12.6.2",
    "@nrwl/workspace": "12.6.2",
